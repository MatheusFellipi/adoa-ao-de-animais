{
  "name": "adocao_de_animais",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:sync": "npm run typeorm schema:sync -- -d src/shared/infra/typeorm",
    "typeorm:drop": "npm run typeorm schema:drop",
    "typeorm:reset": "npm run typeorm:drop && npm run typeorm:sync",
    "typeorm:migrate": "env NODE_ENV=development npm run typeorm migration:generate -- -n",
    "typeorm:create": "yarn typeorm migration:create src/shared/infra/typeorm/migrations/",
    "typeorm:run": "yarn typeorm migration:run -d src/shared/infra/typeorm/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "aws-sdk": "^2.1633.0",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "enum": "^3.0.4",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.0",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "typeorm-pagination": "^2.0.3"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.588.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mocha": "^10.0.6",
    "@types/multer": "^1.4.11",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.12.8",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
